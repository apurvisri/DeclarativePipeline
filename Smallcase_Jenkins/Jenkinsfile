pipeline {
  agent any
  stages {
    stage('Cloning Git') {
      steps {
        git 'https://github.com/apurvisri/Docker.git'
      }
    }
    stage('Building image') {
      steps{
        script {
          dockerImage = docker.build myimage + ":$BUILD_NUMBER" 
        }
      }
    }
     stage('Remove Unused docker image') {
      steps{
        sh "docker rmi $registry:$BUILD_NUMBER"
      }
    }
  }
}
  
